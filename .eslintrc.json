{
    "env": {
      "browser": true,
      "es2021": true
    },
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": ["@typescript-eslint", "simple-import-sort"],
    "rules": {
      "no-console": "warn",
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
    }
  }
  